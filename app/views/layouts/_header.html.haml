-# %script{src:}
.Header
  .Header__inner
    .Header__inner__search
      %h1.Header__inner__search__logo
        = link_to(image_tag('frema-logo-normal.svg'),'#',class: 'Header__inner__search__logo--a')
      .Header__inner__search__Box
        = form_with(url: '/',class: 'Header__inner__search__Box--form')
        = text_field_tag(:search,nil,placeholder:"キーワードから探す",class: 'Header__inner__search__Box--input')
        = button_tag((image_tag 'search-logo.svg'),name: 'search',class: 'Header__inner__search__Box--btn')
      %ul.Heder__inner__search__resultLists
    .Header__inner__nav
      %ul.Header__inner__nav__Llists
        %li.Header__inner__nav__Llists__item
          = link_to('カテゴリー', '#',class: "Header__inner__nav__Llists__item--cat")
          .tree.displayNone
            %ul.tree__categoryTree
              %li.categoryTree--item
                %a#1{href: "#"} レディース
              %li.categoryTree--item
                %a#2{href: "#"} メンズ
              %li.categoryTree--item
                %a#3{href: "#"} ベビー・キッズ
